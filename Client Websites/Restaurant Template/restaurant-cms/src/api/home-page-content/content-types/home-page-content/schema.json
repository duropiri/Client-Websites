{
  "kind": "collectionType",
  "collectionName": "home_page_contents",
  "info": {
    "singularName": "home-page-content",
    "pluralName": "home-page-contents",
    "displayName": "HomePageContent",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "mainDescription": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "default": "We are a locally owned  & Italian owned, casual and authentic Italian restaurant located in the  heart of downtown Saskatoon.  The magic at La Cucina happens in “the kitchen”.  We use simple and fresh ingredients to create classic, home-style Italian dishes."
    },
    "marqueeHeading": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "default": "Saskatoons Premiere Italian Dining Room"
    },
    "heroSubheading": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "default": "Rustic - Authentic - Italian"
    },
    "heroHeading": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "default": "Enjoy a little taste of Italy! Buon Appetito!"
    },
    "marquee": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "centered_contents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::centered-content.centered-content"
    },
    "centered_styled_contents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::centered-styled-content.centered-styled-content"
    }
